{
  "name": "html5-games-workshop",
  "version": "1.0.0",
  "description": "html5 game development workshop",
  "main": "index.js",
  "watch": {
    "build": {
      "patterns": [
        "src"
      ],
      "extensions": "md,css,pug",
      "quiet": true
    }
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "node index.js",
    "build:ghpages": "node index.js --base \"/pizzicato-game.github.io\"",
    "watch": "npm-watch",
    "ghpages": "gh-pages -d dist",
    "deploy": "npm run build:ghpages && npm run ghpages"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pizzicato-game/pizzicato-game.github.io.git"
  },
  "keywords": [
